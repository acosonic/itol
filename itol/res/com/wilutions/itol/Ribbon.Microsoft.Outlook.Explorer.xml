<?xml version="1.0" encoding="utf-8"?>
<customUI onLoad="onLoadRibbon"
	xmlns="http://schemas.microsoft.com/office/2009/07/customui">

	<ribbon>
		<tabs>
			<tab idMso="TabMail">
				<group id="grpIssue" label="Tracker" getVisible="Button_getVisible">
					<!-- Icon from http://www.doublejdesign.co.uk http://www.iconarchive.com/show/childish-icons-by-double-j-design.html -->
					<toggleButton id="NewIssue" tag="Alert-icon-32.png" size="large"
						getLabel="Button_getLabel" onAction="Button_onAction" getEnabled="Button_getEnabled"
						getVisible="Button_getVisible" getImage="getRibbonIcon"
						getPressed="Button_getPressed" />
				</group>
			</tab>
		</tabs>
	</ribbon>


	<backstage>
		<tab id="ItolBackstageTab" label="Issue Tracker">
			<firstColumn>
				<group id="regularGroup" label="Issue Tracker Configuration {1}">
					<bottomItems>
						{0}
					</bottomItems>
				</group>
			</firstColumn>
		</tab>
	</backstage>


</customUI>


