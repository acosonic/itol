<?xml version="1.0" encoding="utf-8"?>
<!-- -->
<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui"
	onLoad="onLoadRibbon">
	<ribbon>
		<tabs>
			<tab idMso="TabReadMessage">
				<group id="grpIssue" label="Tracker" getVisible="Button_getVisible">
					<!-- Icon from http://www.doublejdesign.co.uk http://www.iconarchive.com/show/childish-icons-by-double-j-design.html -->
					<toggleButton id="NewIssue" tag="Alert-icon-32.png"
						size="large" getLabel="Button_getLabel" onAction="Button_onAction"
						getEnabled="Button_getEnabled" getVisible="Button_getVisible"
						getPressed="Button_getPressed" getImage="getRibbonIcon" />
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>