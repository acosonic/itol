<?xml version="1.0" encoding="utf-8"?>
<!-- -->
<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui"
	onLoad="onLoadRibbon">
	<ribbon>
		<tabs>
			<tab idMso="TabNewMailMessage">
				<group id="grpIssue" getLabel="Ribbon_getLabel">
					<!-- Icon from http://www.doublejdesign.co.uk http://www.iconarchive.com/show/childish-icons-by-double-j-design.html -->
					<toggleButton id="bnNewIssue" size="large" getLabel="Ribbon_getLabel"
						onAction="Ribbon_onAction" getEnabled="Ribbon_getEnabled"
						getVisible="Ribbon_getVisible" getImage="Ribbon_getImage"
						getPressed="Ribbon_getPressed" />
				</group>
			</tab>

			
		</tabs>
	</ribbon>
</customUI>